.target{
    box-shadow: 4px 4px 20px rgba(0, 0, 0, 0.25);
    width: 295px;
    height: 225px;
    background-size: auto 100%;
    position: relative;
    overflow: hidden;
  transition: all 225ms ease-in-out;
}
.target:hover{
  transform: scale(1.1);
}
.target ul{
  position: absolute;
  font-size: 14.2px;
  top: 225px;
  width: 295px;
  height: 225px;
  color: #FFF;
  list-style: none;
  padding: 1em 0 1em 0.5em ;
  font-weight: 400;
  box-sizing: border-box;
  left: 0;
  background: rgba(14, 78, 173, 0.73);
  margin: 0;
}
.target ul li{
  margin: 0;
}
.target-name{
  background: rgba(14, 78, 173, 0.73);
  transform: translate(2px);
  color: #FFF;
  font-size: 24px;
  padding: 1em 0 1em 0.5em ;
  font-weight: 400;
  text-transform: uppercase;
  position: absolute;
  bottom: 20px;
  height: 80px;
  width: 246px;
  box-sizing: border-box;
  left: -2px;
}
.target:nth-child(3) .target-name{
  padding: 9px 0 1em 0.5em ; !important;
}
.target-name::after{
  content: "";
  border-bottom: 80px solid rgba(14, 78, 173, 0.73);
  border-right: 20px solid transparent;
  position: absolute;
  bottom: 0;
  left: 246px;
}
